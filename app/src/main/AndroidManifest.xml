<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.landenlabs.all_flipanimation"
    android:installLocation="auto">

    <application
        android:allowBackup="true"
        android:description="@string/app_desc"
        android:icon="@drawable/flipanimation_ic"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.landenlabs.all_flipanimation.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.landenlabs.all_flipanimation.ActivityViewFlipper"
            android:label="@string/app_name" />
        <activity
            android:name="com.landenlabs.all_flipanimation.ActivityRotAnimation"
            android:label="@string/app_name" />
        <activity
            android:name="com.landenlabs.all_flipanimation.ActivityRotAnimComp"
            android:label="@string/app_name" />
        <activity
            android:name="com.landenlabs.all_flipanimation.ActivityObjAnimImg"
            android:label="@string/app_name" />
        <activity
            android:name="com.landenlabs.all_flipanimation.ActivityObjAnimView"
            android:label="@string/app_name" />
        <activity
            android:name="com.landenlabs.all_flipanimation.ActivityObjAnimListRT"
            android:label="@string/app_name" />
        <activity
            android:name="com.landenlabs.all_flipanimation.ActivityObjAnimListR"
            android:label="@string/app_name" />
    </application>

</manifest>
